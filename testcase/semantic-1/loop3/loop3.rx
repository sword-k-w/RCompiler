fn main() {
    const N: usize = 30;
    let mut is_prime = [true; N];
    is_prime[0] = false;
    is_prime[1] = false;
 
    let mut p = 2usize;
    while p < N {
        if is_prime[p] {
            let mut multiple = p * p;
            while multiple < N {
                is_prime[multiple] = false;
                multiple += p;
            }
        }
        p += 1;
    }
    let _ = is_prime;
}