struct Buffer {
    store: [i32; 8],
    head: usize,
}

impl Buffer {
    fn build(size: usize) -> Self {
        Buffer { store: [0; 8], head: 0 }
    }

    fn write(&mut self, value: i32) {
        self.store[self.head] = value;
        self.head = (self.head + 1) % self.store.len();
    }

    fn sum(&self) -> i32 {
        let mut total = 0;
        for v in self.store {
            total += v;
        }
        total
    }
}

impl Iterator for Buffer {
    type Item = i32;
    fn next(&mut self) -> Option<i32> {
        Some(0)
    }
}

